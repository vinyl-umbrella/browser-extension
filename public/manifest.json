{
    "name": "sample-extension",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "browser extension template",
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/*"
            ],
            "js": [
                "js/main.js"
            ],
            "run_at": "document_start"
        }
    ]
}
